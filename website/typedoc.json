{
  /* https://typedoc.org/guides/options/ */
  "$schema": "https://typedoc.org/schema.json",
  "cleanOutputDir": true,
  "entryPointStrategy": "resolve",
  "entryPoints": [
    "../src/index.ts"
  ],
  "logLevel": "Verbose",
  "out": "./docs/api",
  "plugin": ["typedoc-plugin-markdown"],
  "readme": "none",
  "skipErrorChecking": true,
  "sort": [
    "instance-first",
    "visibility"
  ],
  "tsconfig": "../src/tsconfig.json"
}
